    "description": "Pyncer library for working with search.",
    "homepage": "https://pyncer.com/search",
    "license": "Unlicense",
    "require": {
        "php": ">=8.3",
        "pyncer/core": "^1.3",
        "pyncer/exception": "^1.0",
        "voku/portable-ascii": "^2.0"
